{
  "devDependencies": {
    "html-minifier": "^0.7.0"
  },
  "private": true,
  "scripts": {
    "build": "npm run build-template",
    "build-template": "node_modules/html-minifier/cli.js --collapse-whitespace --lint --remove-comments --remove-comments-from-cdata -o dist/main.html src/main.html",
    "clean": "rm -fr dist/*"
  }
}
